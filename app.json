{
  "expo": {
    "name": "cryptal-app",
    "slug": "cryptal-app",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "assets/images/*",
      "assets/fonts/*"
    ],
    "description": "",
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow cryptal-app to access your photos.",
